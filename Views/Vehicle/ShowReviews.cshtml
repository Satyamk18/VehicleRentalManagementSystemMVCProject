@model IEnumerable<VehicleRentalManagementProject.Models.Review>;
@* 
@{
    List<Review> reviews = ViewBag.reviews?List<Review>();
} *@

<h4 style="text-align:center">Reviews for @ViewBag.VehicleName</h4>
<br>
<table class="table table-hover table-bordered">
    <thead>
    <tr>
        <th>User Id</th>
        <th>Review</th>
    </tr>
    </thead>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Userid</td>
            <td>@item.Review1</td>
        </tr>
    }
</table>
<div>
 <button type="button" class="btn btn-dark"><a asp-action="ShowVehicles" style="color:white">Back to List</a></button>
</div>